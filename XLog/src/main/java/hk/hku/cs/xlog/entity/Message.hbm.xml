<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="hk.hku.cs.xlog.entity.Message" table="message">
		<id name="messageId" column="messageId" type="string">
			<generator class="assigned" />
		</id>
		<property name="idAtService" column="idAtService" type="string" />
		<property name="refUser" column="refUser" type="string" />
		<property name="serviceProvider" column="serviceProvider" type="string" />
		<property name="fromName" column="fromName" type="string" />
		<property name="fromEmail" column="fromEmail" type="string" />
		<property name="fromId" column="fromId" type="string" />
		<property name="fromProfileImage" column="fromProfileImage" type="string" />
		<property name="toName" column="toName" type="string" />
		<property name="toEmail" column="toEmail" type="string" />
		<property name="toId" column="toId" type="string" />
		<property name="toProfileImage" column="toProfileImage" type="string" />
		<property name="content" column="content" type="string" />
		<property name="createdDate" column="createdDate" type="date" />
		<property name="subject" column="subject" type="string" />
		<property name="attachedFile" column="attachedFile" type="string" />
		<property name="xread" column="xread" type="boolean" />

	</class>

</hibernate-mapping>